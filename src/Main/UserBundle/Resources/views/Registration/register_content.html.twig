{% trans_default_domain 'FOSUserBundle' %}
<h1 style="text-align:center;"> Inscription </h1>
<div class="container blocCentre">
	<div class="row">
		<div class="col-xs-6 col-lg-6 col-lg-offset-3">
			<form action="{{ path(app.request.attributes.get('_route')) }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
				{{ form_widget(form) }}
				<div>
					<input type="submit" class="btn btn-info" value="{{ 'registration.submit'|trans }}" />
				</div>
			</form>
		</div>
	</div>
</div>
